    "tslint": "~6.1.3",
    "tstest": "^0.4.10",
    "typescript": "~4.0.5",
    "zone.js": "~0.11.3"
  },
  "peerDependencies": {
    "brolog": ">=1",
    "rxjs": ">=6",
    "state-switch": "*"
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    },
    "whitelistedNonPeerDependencies": []
  },
  "git": {
    "scripts": {
      "pre-push": "npx git-scripts-pre-push"
    }
