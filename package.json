{
  "name": "@chatie/angular",
  "version": "0.4.8",
  "description": "Wechaty Component NgModule",
  "license": "Apache-2.0",
  "scripts": {
    "build": "ng build",
    "clean": "shx rm -fr dist/* aot/* bundles/*",
    "lint": "npm run lint:ts && npm run lint:ng && npm run ngc",
    "lint:ng": "ng lint",
    "lint:ts": "tslint --project tsconfig.json && tsc --noEmit",
    "ng": "ng",
    "ng:packagr": "ng-packagr -p package.json",
    "ngc": "ngc -p src/tsconfig.aot.json",
    "start": "ng serve",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "test:e2e": "ng e2e",
    "test:unit": "ng test --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chatie/angular.git"
  },
  "keywords": [
    "angular",
    "ngmodule",
    "chatbot",
    "chatie",
    "wechat",
    "wechaty"
  ],
  "author": "Huan LI <zixia@zixia.net>",
  "bugs": {
    "url": "https://github.com/chatie/angular/issues"
  },
  "homepage": "https://github.com/chatie/angular#readme",
  "devDependencies": {
    "@angular/cli": "^8.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "@types/jasmine": "^3.0.0",
    "@types/node": "^12.0.4",
    "brolog": "^1.2.17",
    "codelyzer": "^4.2.1",
    "core-js": "^3.0.0",
    "git-scripts": "^0.2.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma-chrome-launcher": "~3.0.0",
    "karma": "~4.2.0",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~2.0.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "ng-packagr": "^5.0.1",
    "protractor": "~6.0.0",
    "puppeteer": "^1.3.0",
    "rxjs": "^6.0.0",
    "shx": "^0.3.0",
    "state-switch": "^0.6.2",
    "tsickle": "^0.36.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.9.1",
    "typescript": "2.7.2",
    "uglify-js": "^3.3.21",
    "zone.js": "^0.8.4",
    "@angular-devkit/build-angular": "~0.801.0"
  },
  "peerDependencies": {
    "brolog": ">=1",
    "rxjs": ">=6",
    "state-switch": "*"
  },
  "git": {
    "scripts": {
      "pre-push": "bash ./scripts/pre-push.sh"
    }
  },
  "dependencies": {},
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "public_api.ts"
    },
    "whitelistedNonPeerDependencies": []
  }
}
